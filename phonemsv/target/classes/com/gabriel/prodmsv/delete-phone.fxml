<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>


<Pane prefHeight="600.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gabriel.prodmsv.DeletePhoneController">
   <children>
      <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Delete Item" textAlignment="CENTER" textFill="#e12323">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <ImageView fx:id="contactImage" fitHeight="166.0" fitWidth="187.0" layoutX="87.0" layoutY="57.0" pickOnBounds="true" preserveRatio="true" />
      <HBox layoutX="21.0" layoutY="282.0" prefHeight="35.0" prefWidth="252.0">
         <children>
            <Label prefWidth="100.0" text="Name">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfName" editable="false" />
         </children>
      </HBox>
      <HBox layoutX="21.0" layoutY="317.0" prefHeight="26.0" prefWidth="252.0">
         <children>
            <Label prefWidth="100.0" text="Phone Number">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfPhoneNumber" editable="false" />
         </children>
      </HBox>
      <HBox layoutX="22.0" layoutY="451.0" prefHeight="26.0" prefWidth="252.0">
         <children>
            <Label prefWidth="100.0" text="Group">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfGroup" editable="false" />
         </children>
      </HBox>
      <HBox layoutX="22.0" layoutY="385.0" prefHeight="26.0" prefWidth="252.0">
         <children>
            <Label prefWidth="100.0" text="Social Media:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfSocial" editable="false" />
         </children>
      </HBox>
      <GridPane layoutX="80.0" layoutY="527.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnBack" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onBack" prefWidth="90.0" text="&lt;&lt; Back" />
            <Button fx:id="btnSubmit" mnemonicParsing="false" onAction="#onSubmit" prefWidth="90.0" text="Submit" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <HBox layoutX="22.0" layoutY="352.0" prefHeight="26.0" prefWidth="252.0">
         <children>
            <Label prefWidth="100.0" text="Email">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfEmail" editable="false" />
         </children>
      </HBox>
      <HBox layoutX="21.0" layoutY="487.0" prefHeight="26.0" prefWidth="252.0">
         <children>
            <Label prefWidth="100.0" text="Birthday">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfBirthDate" editable="false" />
         </children>
      </HBox>
      <HBox layoutX="22.0" layoutY="418.0" prefHeight="26.0" prefWidth="252.0">
         <children>
            <Label prefWidth="100.0" text="Account Name:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfAccount" editable="false" />
         </children>
      </HBox>
      <Label layoutX="160.0" layoutY="223.0" prefHeight="17.0" prefWidth="41.0" text="Image" />
   </children>
</Pane>
